{
  "name": "ask-gram",
  "private": true,
  "sideEffects": true,
  "type": "module",
  "scripts": {
    "dev": "remix dev --manual",
    "dev:https": "remix dev -c \"node ./server.js\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "build": "npm run build:local",
    "build:local": "remix build",
    "build:dev": "npm run copy:dev && remix build",
    "build:prod": "npm run copy:prod && remix build",
    "start": "remix-serve ./build/index.js",
    "copy:dev": "copyfiles -u 3 app/config/dev/*.ts app/config ",
    "copy:prod": "copyfiles -u 3 app/config/prod/*.ts app/config",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.511.0",
    "@headlessui/react": "^1.7.18",
    "@lexical/react": "^0.14.3",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/express": "^2.7.2",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "feed": "^4.2.2",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.5.1",
    "isbot": "^4.1.0",
    "katex": "^0.16.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.0",
    "remix-i18next": "^6.1.0",
    "rimraf": "^5.0.5",
    "schema-dts": "^1.1.2",
    "tiny-invariant": "^1.3.1",
    "usehooks-ts": "^2.15.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@types/express": "^4.17.21",
    "@types/google.accounts": "^0.0.14",
    "@types/gtag.js": "^0.0.19",
    "@types/katex": "^0.16.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-google-recaptcha": "^2.1.9",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
